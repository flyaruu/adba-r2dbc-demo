<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Blocking</servlet-name>
		<servlet-class>com.dexels.blocking.SQLServletBlocking</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SQLServletSimpleABDABlocking</servlet-name>
		<servlet-class>com.dexels.adba.SQLServletSimpleABDABlocking</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SQLServletABDABlocking</servlet-name>
		<servlet-class>com.dexels.adba.SQLServletABDABlocking</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SQLServletABDANonBlocking</servlet-name>
		<servlet-class>com.dexels.adba.SQLServletABDANonBlocking</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>SQLServletABDANonBlockingBackpressure</servlet-name>
		<servlet-class>com.dexels.adba.SQLServletABDANonBlockingBackpressure</servlet-class>
		<async-supported>true</async-supported>
	</servlet>	
	<servlet>
		<servlet-name>SQLServletR2DBC</servlet-name>
		<servlet-class>com.dexels.r2dbc.SQLServletR2DBC</servlet-class>
		<async-supported>true</async-supported>
	</servlet>	
	<servlet-mapping>
		<servlet-name>Blocking</servlet-name>
		<url-pattern>/Blocking</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SQLServletSimpleABDABlocking</servlet-name>
		<url-pattern>/SQLServletSimpleABDABlocking</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SQLServletABDABlocking</servlet-name>
		<url-pattern>/SQLServletABDABlocking</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SQLServletABDANonBlocking</servlet-name>
		<url-pattern>/SQLServletABDANonBlocking</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SQLServletABDANonBlockingBackpressure</servlet-name>
		<url-pattern>/SQLServletABDANonBlockingBackpressure</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SQLServletR2DBC</servlet-name>
		<url-pattern>/SQLServletR2DBC</url-pattern>
	</servlet-mapping>
</web-app>
